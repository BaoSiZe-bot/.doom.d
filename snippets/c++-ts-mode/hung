# -*- mode: snippet -*-
# key: hung
# name: hungarian
# --
int mp[${1:205}][$1], md[$1], vs[$1], n;
inline bool Hungarian(int x)
{
    for (int i = 1; i <= n; ++i)
        if (mp[x][i] && !vs[i])
        {
            vs[i] = 1;
            if (!md[i] || Hungarian(md[i]))
            {
                md[i] = x;
                return 1;
            }
        }
    return 0;
}
